@model IEnumerable<ASPNetSocialMedia.Models.Post>

@{
    ViewData["Title"] = "Feed";
}

<h1>@ViewData["Title"]</h1>

@foreach (var item in Model)
{
    <div class="container">
        <div class="card" style="margin:10px">
            <div class="card-body">
                <p class="card-text">@Html.DisplayFor(modelItem => item.WhoPosted)</p>
            </div>
            <div class="card-body">
                <p class="card-text">@Html.DisplayFor(modelItem => item.PostContent)</p>
            </div>
        </div>
    </div>
    
}